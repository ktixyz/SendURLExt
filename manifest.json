{
    "manifest_version": 2,
    "name": "Send URL",
    "version": "1.0",
    "description": "Send current tab url to external server",
    "homepage_url": "https://github.com/ktixyz/SendURLExt",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/beasts-32.png",
        "default_title": "Send URL",
        "default_popup": "popup/popup.html"
    }
}