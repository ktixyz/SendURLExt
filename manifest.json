{
    "manifest_version": 2,
    "name": "Send URL",
    "version": "1.0",
    "description": "Send current tab url to external server",
    "permissions": [
        "activeTab",
        "contextMenus",
        "tabs"
    ],
    "icons": {
        "48": "icons/icon.png",
        "512": "icons/icon.png"
    },
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "background.js"
        ]
    }
}